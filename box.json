{
  "compression": "GZ",
  "files": [
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "demo",
        "Demo",
        "Examples",
        "test",
        "tests",
        "Test",
        "Tests",
        "unitTests"
      ],
      "in": [
        "src",
        "vendor/composer",
        "vendor/erusev/parsedown",
        "vendor/symfony"
      ]
    }
  ],
  "output": "htmarkdown.phar",
  "shebang": false,
  "main": "src/index.php",
  "stub": true
}
